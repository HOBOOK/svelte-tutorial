{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prepare": "npm run styles-light && npm run styles-dark",
    "styles-light": "smui-theme compile static/site.css -i style",
    "styles-dark": "smui-theme compile static/site-dark.css -i style/dark",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "node-sass": "^7.0.1",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "smui-theme": "^6.0.0-beta.16",
    "svelte": "^3.0.0",
    "svelte-preprocess": "^4.10.6",
    "svelte-spa-router": "^3.2.0"
  },
  "dependencies": {
    "sirv-cli": "^2.0.0",
    "svelte-material-icons": "^2.0.2",
    "svelte-material-ui": "^6.0.0-beta.16"
  }
}
